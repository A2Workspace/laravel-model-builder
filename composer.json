{
  "name": "a2workspace/laravel-model-builder",
  "description": "提供 Laravel 建立複雜資料的模型生成器",
  "license": "MIT",
  "authors": [
    {
      "name": "Shishamou",
      "email": "shishatw225@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "6.x"
  },
  "autoload": {
    "psr-4": {
      "A2Workspace\\ModelBuilder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "A2Workspace\\ModelBuilder\\ServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ],
    "test-coverage": [
      "vendor/bin/phpunit --coverage-html coverage"
    ]
  }
}
